    "@lrc-player/parse": "^1.0.0",
    "axios": "^1.7.4",
    "colorthief": "^2.6.0",
    "dayjs": "^1.11.13",
    "electron-updater": "^6.1.7",
    "element-plus": "^2.8.0",
    "file-type": "^19.5.0",
    "gsap": "^3.12.5",
    "highlight.js": "^11.10.0",
    "markdown-it": "^14.1.0",
    "pinia": "^2.2.1",
    "pinia-plugin-persistedstate": "^3.2.1",
    "sqlite3": "^5.1.7",
    "typescript": "^5.5.4",
    "uuid": "^10.0.0",
    "vue-router": "^4.4.3",
