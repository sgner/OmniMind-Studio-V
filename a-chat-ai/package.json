    "@vue/eslint-config-prettier": "^9.0.0",
    "electron": "^31.0.2",
    "electron-builder": "^24.13.3",
    "electron-store": "^7.0.0",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.26.0",
    "less": "^4.2.0",
    "prettier": "^3.3.2",
    "sass-embedded": "^1.77.8",
    "vite": "^5.3.1",
    "vue": "^3.4.30"
  }
